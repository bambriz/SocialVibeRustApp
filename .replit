modules = ["rust", "python-3.11", "rust-stable", "python3"]
[agent]
expertMode = true

[nix]
channel = "stable-25_05"
packages = ["freetype", "geckodriver", "glibcLocales", "lcms2", "libGL", "libGLU", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "openssl", "pkg-config", "tcl", "tk", "xsimd", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Server"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "PythonServer"

[[workflows.workflow]]
name = "Server"
author = "agent"

[workflows.workflow.metadata]
outputType = "console"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "/nix/store/bqwc4hzm2lvshv5i3y9l3rmsb17z9a57-rust-mixed/bin/cargo run"

[[workflows.workflow]]
name = "PythonServer"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 python_scripts/async_server.py"

